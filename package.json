{
  "name": "nebula-components",
  "version": "0.2.2",
  "description": "Building on top of Nebula CSS a series of common components",
  "repository": "https://github.com/rbrtsmith/nebula-components",
  "scripts": {
    "lint:scss": "stylelint '*/**/*.scss' --syntax scss",
    "lint:js": "cd demo && npm run lint",
    "lint": "npm run lint:scss && npm run lint:js",
    "build": "cd demo && npm run build",
    "start": "cd demo && npm run start",
    "test": "npm run lint",
    "deploy": "npm run lint && npm run build && gh-pages -d demo/dist",
    "prepublish": "npm run build"
  },
  "keywords": [
    "itcss",
    "bemit",
    "bem",
    "sass",
    "oocss",
    "grid",
    "css"
  ],
  "author": "Robert Smith",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "gh-pages": "^1.0.0",
    "http-server": "^0.10.0",
    "node-sass": "^4.5.3",
    "onchange": "^3.2.1",
    "postcss-cli": "^4.1.0",
    "stylelint": "^8.0.0",
    "stylelint-config-nebula": "^0.2.0"
  },
  "dependencies": {
    "nebula-css": "^2.7.0"
  }
}
